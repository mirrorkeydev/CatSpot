<div class='cat'>
  <div class='cat-photo'>
    <img src="{{imageName}}" alt="{{color}}Cat">
  </div>
  <h2 class='cat-title'> {{color}} cat </h2>
  <div class='element'>
    <h3 class='timeline-element-title'> Energy: </h2>
    <h3 class='timeline-element-text'> {{energy}} </h3>
  </div>
  <div class='element'>
    <h3 class='timeline-element-title'> Sociability: </h2>
    <h3 class='timeline-element-text'> {{sociability}} </h3>
  </div>
  <div class='element'>
    <h3 class='timeline-element-title'> Color: </h2>
    <h3 class='timeline-element-text'> {{color}} </h3>
  </div>
</div>
